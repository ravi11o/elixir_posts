<div class="container">
  <div class="center">
    <h4>Select by Year</h4>
  </div>
  <div class="row">
    <%= for x <- 2015..2017 do %>
      <div class="col s4">
        <a href="/conferences/<%= x %>">
         <div class="card blue-grey darken-1">
           <div class="card-content white-text">
             <span class="card-title center"><%= x %></span>
           </div>
         </div>
       </a>
      </div>
    <% end %>
  </div>
</div>
