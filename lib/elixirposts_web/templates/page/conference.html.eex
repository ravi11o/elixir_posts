<div class="container">
  <div class="row">
    <%= for x <- 2014..2017 do %>

     <div class="col s4">
       <a href="/conferences/<%= x %>">
         <div class="card blue-grey darken-1">
           <div class="card-content white-text">
             <span class="card-title center"><%= x %></span>
           </div>
         </div>
       </a>
      </div>
    <% end %>
  </div>
</div>
